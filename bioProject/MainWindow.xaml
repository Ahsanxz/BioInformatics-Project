        xmlns:local="clr-namespace:bioProject"
        mc:Ignorable="d"
        Title="MainWindow" Height="494" Width="800">
    
    <Grid>
        <ScrollViewer Margin="0,0,0,129">

            <TextBlock Name="outputText" Margin="0,15,0,0" TextWrapping="Wrap" Width="765" Cursor="" Background="{DynamicResource {x:Static SystemColors.ControlLightBrushKey}}"/>

        </ScrollViewer>
        <Button Content="Select File" HorizontalAlignment="Left" Margin="103,425,0,0" VerticalAlignment="Top" Height="30" Width="60" Click="Button_Click"/>
        <Button Name="startBtn" Content="Start Alignment" HorizontalAlignment="Center" Margin="0,354,0,0" VerticalAlignment="Top" Width="100" Height="40" Click="Button_Click_1" IsEnabled="False"/>
        <Label Name="fileNametxt" Content="File Directory" BorderBrush="Black"  BorderThickness="0.5" HorizontalAlignment="Center" Margin="0,425,0,0" VerticalAlignment="Top" Height="30" Width="392"/>
     


    </Grid>
